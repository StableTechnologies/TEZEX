{
  "name": "client",
  "version": "0.2.0",
  "private": true,
  "dependencies": {
    "@airgap/beacon-sdk": "^2.3.13",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@mui/core": "^5.0.0-alpha.54",
    "@mui/icons-material": "^5.6.2",
    "@mui/material": "^5.0.1",
    "@mui/styles": "^5.0.1",
    "@mui/system": "^5.8.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "async-mutex": "^0.3.2",
    "big-integer": "1.6.51",
    "bignumber.js": "^9.0.1",
    "clsx": "^1.1.1",
    "conseiljs": "5.2.3",
    "jsonpath-plus": "6.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.5",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^5.0.0",
    "redux": "^4.1.2",
    "redux-thunk": "^2.3.0",
    "tezoslendingplatformjs": "../util/",
    "web-vitals": "^1.1.2"
  },
  "devDependencies": {
    "@types/node": "^16.11.10",
    "@types/node-fetch": "^3.0.3",
    "@typescript-eslint/parser": "^5.4.0",
    "assert": "^2.0.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "https-browserify": "^1.0.0",
    "loglevel": "^1.8.0",
    "node-fetch": "^3.2.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "react-app-rewired": "^2.1.11",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "typescript": "^4.5.2",
    "url": "^0.11.0"
  },
  "scripts": {
    "start": "DISABLE_ESLINT_PLUGIN=true REACT_APP_ENV=dev react-app-rewired start",
    "mainnet": "DISABLE_ESLINT_PLUGIN=true react-app-rewired start",
    "build": "tsc && react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "format": "eslint --fix src"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "env": {
      "mocha": true,
      "node": true
    },
    "overrides": [
      {
        "files": [
          "**/*.ts",
          "**/*.js"
        ],
        "excludedFiles": []
      }
    ],
    "extends": "airbnb-base",
    "rules": {
      "comma-dangle": [
        2,
        "never"
      ],
      "curly": [
        2,
        "all"
      ],
      "indent": [
        1,
        4
      ],
      "max-len": [
        2,
        {
          "code": 240,
          "ignoreTrailingComments": true
        }
      ],
      "no-tabs": [
        2
      ],
      "quotes": [
        2,
        "single"
      ],
      "array-element-newline": [
        2,
        {
          "multiline": true,
          "minItems": 5
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}